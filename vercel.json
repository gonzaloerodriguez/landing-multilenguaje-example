{
    "version": 2,
    "builds": [
      { "src": "*.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/img/(.*)\\.svg", "dest": "/img/$1.svg" },
      { "src": "/img/(.*)", "dest": "/img/$1" },
      { "src": "/(.*)\\.js", "dest": "/$1.js" },
      { "src": "/(.*)\\.css", "dest": "/$1.css" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }